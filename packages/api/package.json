    "@babel/node": "^7.12.6",
    "@babel/preset-env": "^7.12.7",
    "babel-jest": "^26.6.3",
    "jest": "^26.6.3",
    "jest-cli": "^26.6.3",
    "nodemon": "^2.0.6"
  },
  "dependencies": {
    "fastify": "^3.8.0",
    "knex": "^0.21.12",
    "lodash": "^4.17.20",
    "objection": "^2.2.3",
    "pg": "^8.5.1"
  }
}